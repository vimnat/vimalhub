package org.greenstech;

public class ArrayDup {
	
	static int removeDuplicates(int arr[], int n) 
    { 
        if (n==0 || n==1) 
            return n; 
       
        int[] temp = new int[n]; 
          
        int j = 0; 
        for (int i=0; i<n-1; i++) {

            if (arr[i] != arr[i+1]) {
                temp[j++] = arr[i]; 
            }
        }
        temp[j++] = arr[n-1];   
        System.out.println(j);
          
        for (int i=0; i<j; i++) 
            arr[i] = temp[i]; 
       
        return j; 
    } 
      
    public static void main (String[] args)  
    { 
        int arr[] = {10,10,20,50,60,80,60,50}; 
        int n = arr.length; 
          
        n = removeDuplicates(arr, n); 
   

        for (int i=0; i<n; i++) 
           System.out.print(arr[i]+" "); 
        
    }
}
